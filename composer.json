{
	"name": "reliqarts/laravel-non-www",
	"description": "Tiny Laravel 5 middleware to redirect www requests to non-www counterparts.",
	"keywords": ["laravel", "non", "www", "redirect"],
	"type": "package",
	"license": "MIT",
	"authors": [
		{
			"name": "ReliQ",
			"email": "reliq@reliqarts.com"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"illuminate/support": "5.*"
	},
	"autoload": {
		"psr-4": {
			"ReliQArts\\NonWWW\\": "src/"
		}
	},
	"extra": {
        "laravel": {
            "providers": [
                "ReliQArts\\NonWWW\\NonWWWServiceProvider"
            ]
        }
    }
}
