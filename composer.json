{
    "name": "reliqarts/laravel-non-www",
    "description": "Tiny Laravel middleware to redirect www requests to non-www counterparts.",
    "keywords": ["laravel", "non", "www", "redirect"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Reliq",
            "email": "reliq@reliqarts.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0",
        "fideloper/proxy": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ReliqArts\\NonWWW\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["ReliqArts\\NonWWW\\ServiceProvider"]
        }
    }
}
